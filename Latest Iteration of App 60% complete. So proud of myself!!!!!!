    <script>
         
            function Tech(name, skill, turn) {
                this.name = name;
                this.skill = skill;
                this.turn = turn;
                this.updateTurn = function () {
                    return ++this.turn;
                };
            }

                let person01 = new Tech("Cindy", 1, 0);
                let person02 = new Tech("Vi", 2, 0);
                let person03 = new Tech("Khoa", 2, 0);
                let person04 = new Tech("Pauline", 3, 0);
                let person05 = new Tech("Linh", 2, 0);
                let person06 = new Tech("Lee", 3, 0);
                let person07 = new Tech("John", 2, 0);
                let person08 = new Tech("David", 2, 0);
                let person09 = new Tech("Will", 1, 0);

            let arr = [];

            $('#btn1').click(function(){
                arr.push(person01);
                    //console.log("Cindy", 0);
                    document.getElementById("array").innerHTML = "Cindy";
            });
            $('#btn2').click(function () {
                arr.push(person02)
            });
            $('#btn3').click(function () {
                arr.push(person03)
            });
            $('#btn4').click(function () {
                arr.push(person04)
            });
            $('#btn5').click(function () {
                arr.push(person05)
            });
            $('#btn6').click(function () {
                arr.push(person06)
            });
            $('#btn7').click(function () {
                arr.push(person07)
            });
            $('#btn8').click(function () {
                arr.push(person08)
            });
            $('#btn9').click(function () {
                arr.push(person09)
            });
            $('#btn10').click(function () {
                console.log(arr)
            });



            


